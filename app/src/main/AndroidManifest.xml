<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.portfolio">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cat_pc"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/cat_pc"
        android:supportsRtl="true"
        android:theme="@style/Theme.Portfolio">
        <activity android:name=".StoreDataProjectManager"></activity>
        <activity android:name=".NetworkingRetrofitMovies" />
        <activity android:name=".StylesBatteryControl" />
        <activity android:name=".ViewGroupsScrollArray" />
        <activity android:name=".ViewsChallange" />
        <activity android:name=".ViewGroupsLogin" />
        <activity
            android:name=".Register"
            android:theme="@style/Theme.NoActionBar" />
        <activity
            android:name=".Login"
            android:theme="@style/Theme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>